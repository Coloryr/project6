<Window x:Class="ColoryrTrash.Desktop.Windows.GroupChose"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ColoryrTrash.Desktop.Windows"
        mc:Ignorable="d"
        Title="修改组" Height="85" Width="282" ResizeMode="NoResize">
    <Grid>
        <Rectangle Fill="#FFF4F4F5" Stroke="#FF5ABED6" StrokeThickness="2"/>
        <Label Content="UUID:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="2,2,0,0"/>
        <ComboBox Margin="48,30,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Height="20" Width="167" Name="GroupList" SelectionChanged="GroupList_SelectionChanged"/>
        <Button Content="确定" Margin="220,30,0,0" Height="20" VerticalAlignment="Top" HorizontalAlignment="Left" Width="50" Click="Button_Click"/>
        <Label Content="选择组" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="2,27,0,0"/>
        <Label Content="{Binding UUID}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="48,2,0,0" Height="25"/>
    </Grid>
</Window>
